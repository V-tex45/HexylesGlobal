{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/public/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1.html",
      "status": 200
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://api.hexylesglobal.com",
    "NEXT_PUBLIC_GOOGLE_MAPS_KEY": "@hexyles-google-maps-key"
  }
}